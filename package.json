{
  "name": "allmight",
  "version": "0.0.1",
  "bin": {
    "allmight": "./bin/allmight.js"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "lint": "standard",
    "build": "chmod 755 ./bin/*",
    "echo": "echo 'this is from allmight echo!'",
    "prestart": "echo 'pre start!!!' && watchman watch-del-all",
    "preios": "echo 'pre ios!!!' && cd ios && pod install --repo-update && pod install && cd .. && watchman watch-del-all",
    "preandroid": "echo 'pre android!!!' && watchman watch-del-all",
    "open-ios": "open ./ios/allmight.xcworkspace",
    "postrename": "cd ios && pod install --repo-update && pod install && cd .. && watchman watch-del-all"
  },
  "dependencies": {
    "@babel/core": "^7.7.5",
    "@babel/runtime": "^7.7.6",
    "@react-native-community/netinfo": "^5.0.1",
