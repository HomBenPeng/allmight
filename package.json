    "appcenter-analytics": "2.6.1",
    "appcenter-crashes": "2.6.1",
    "appcenter-push": "2.6.1",
    "arg": "^4.1.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "cheerio": "^0.22.0",
    "colors": "^1.1.2",
    "fs-extra": "^8.1.0",
    "metro-react-native-babel-preset": "^0.57.0",
    "native-base": "^2.13.8",
